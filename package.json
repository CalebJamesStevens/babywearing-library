{
  "name": "babywearing-library",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run --workspaces --if-present dev",
    "dev:web": "npm --workspace @babywearing/web run dev",
    "dev:admin": "npm --workspace @babywearing/admin run dev",
    "build": "npm run --workspaces --if-present build",
    "build:web": "npm --workspace @babywearing/web run build",
    "build:admin": "npm --workspace @babywearing/admin run build",
    "lint": "npm run --workspaces --if-present lint",
    "db:generate": "npm --workspace @babywearing/db run generate",
    "db:migrate": "npm --workspace @babywearing/db run migrate",
    "db:studio": "npm --workspace @babywearing/db run studio"
  },
  "dependencies": {
    "ua-parser-js": "2.0.9"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
